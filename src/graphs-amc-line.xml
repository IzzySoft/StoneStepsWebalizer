<?xml version="1.0" encoding="utf-8"?>
<!-- 
   For complete amCharts documentation, please refer to this page:
   
   http://www.amcharts.com/docs/v.1/bundle/settings/line_area 
-->
<settings>
   <data_type>xml</data_type>"
   <decimals_separator>.</decimals_separator>
   <thousands_separator>none</thousands_separator>
   <digits_after_decimal>0</digits_after_decimal>
   <rescale_on_hide>false</rescale_on_hide>

   <background>
      <!-- transparent background -->
      <alpha>0</alpha>
   </background>

   <grid>
      <x>
         <!-- use the highest value for all line graphs (e.g. 24 hours vs. 31 days)-->
         <approx_count>31</approx_count>
      </x>
      <y_left></y_left>
      <y_right>
         <enabled>false</enabled>
      </y_right>
   </grid>

   <plot_area>
      <color></color>
      <margins>
         <top>45</top>
         <right>50</right>
         <bottom>50</bottom>
         <left>50</left>
      </margins>
   </plot_area>

   <values> 
      <x> 
         <frequency>1</frequency> 
         <color>#000000</color> 
         <text_size></text_size> 
      </x> 
      <y_left> 
         <frequency></frequency> 
         <color></color> 
         <text_size></text_size> 
      </y_left> 
      <y_right> 
         <frequency></frequency> 
         <color></color> 
         <text_size></text_size> 
      </y_right> 
   </values>

   <axes>
      <x>
         <color></color>
         <width>1</width>
         <tick_length>4</tick_length>
      </x>
      <y_left>
         <color></color>
         <width>1</width>
         <tick_length>4</tick_length>
         <logarithmic></logarithmic>
      </y_left>
      <y_right>
         <color></color>
         <width>1</width>
         <tick_length>4</tick_length>
      </y_right>
   </axes>

   <legend>
      <text_color></text_color>
      <text_color_hover>#FF0000</text_color_hover>
      <text_size></text_size>
      <spacing></spacing>
      <margins></margins>
      <align>center</align>
      <max_columns>5</max_columns>
      <key>
         <size></size>
         <border_color>#000000</border_color>
         <key_mark_color>#FFFFFF</key_mark_color>
      </key>
      <values>
         <enabled>true</enabled>
         <align>left</align>
         <text><![CDATA[: {value}]]></text>
      </values>
   </legend>

   <indicator>
      <enabled>true</enabled>
      <zoomable></zoomable>
      <color>#FF7777</color>
      <line_alpha>100</line_alpha>
      <selection_color>#FF0000</selection_color>
      <selection_alpha>15</selection_alpha>
      <x_balloon_enabled>true</x_balloon_enabled>
   </indicator>
     
   <balloon>
      <enabled>true</enabled> 
      <color>#FFFFFF</color> 
      <alpha>85</alpha> 
      <only_one>true</only_one>
      <text_color>#000000</text_color> 
      <text_size></text_size> 
      <corner_radius>2</corner_radius> 
      <border_width>1</border_width> 
      <border_alpha>75</border_alpha> 
      <border_color>#000000</border_color> 
   </balloon> 

   <vertical_lines>
      <width>60</width>
      <clustered>true</clustered>
      <alpha>100</alpha>
      <mask>false</mask>
   </vertical_lines>

   <labels> 
      <label lid="graph_title">
         <align>center</align>
         <text_size>14</text_size>
      </label> 
   </labels>

   <graphs>
      <!-- hits (area) -->
      <graph gid="1">
         <axis>left</axis> 
         <line_alpha>100</line_alpha> 
         <line_width>2</line_width> 
         <bullet>round_outline</bullet> 
         <bullet_size>5</bullet_size> 
         <bullet_color>#000000</bullet_color> 
         <fill_alpha>100</fill_alpha> 
         <vertical_lines>false</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- files (area) -->
      <graph gid="2">
         <axis>left</axis> 
         <line_alpha>100</line_alpha> 
         <line_width>2</line_width> 
         <bullet>round_outline</bullet> 
         <bullet_size>5</bullet_size> 
         <bullet_color>#000000</bullet_color> 
         <fill_alpha>100</fill_alpha> 
         <vertical_lines>false</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- pages (area) -->
      <graph gid="3">
         <axis>left</axis> 
         <line_alpha>100</line_alpha> 
         <line_width>2</line_width> 
         <bullet>round_outline</bullet> 
         <bullet_size>5</bullet_size> 
         <bullet_color>#000000</bullet_color> 
         <fill_alpha>100</fill_alpha> 
         <vertical_lines>false</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- hosts (line) -->
      <graph gid="4">
         <axis>right</axis> 
         <line_width>2</line_width> 
         <bullet>round_outlined</bullet> 
         <color_hover>#FF9999</color_hover>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- visits (line) -->
      <graph gid="5">
         <axis>right</axis> 
         <line_width>2</line_width> 
         <bullet>round_outlined</bullet> 
         <color_hover>#FF9999</color_hover>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- transfer (line) -->
      <graph gid="6">
         <axis>right</axis> 
         <line_width>2</line_width> 
         <bullet>round_outlined</bullet> 
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- hits (column) -->
      <graph gid="7">
         <axis>left</axis> 
         <line_alpha>0</line_alpha> 
         <line_width>2</line_width> 
         <bullet></bullet> 
         <fill_alpha>0</fill_alpha> 
         <vertical_lines>true</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- files (column) -->
      <graph gid="8">
         <axis>left</axis> 
         <line_alpha>0</line_alpha> 
         <line_width>2</line_width> 
         <bullet></bullet> 
         <fill_alpha>0</fill_alpha> 
         <vertical_lines>true</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

      <!-- pages (column) -->
      <graph gid="9">
         <axis>left</axis> 
         <line_alpha>0</line_alpha> 
         <line_width>2</line_width> 
         <bullet></bullet> 
         <fill_alpha>0</fill_alpha> 
         <vertical_lines>true</vertical_lines>
         <balloon_text_color>#000000</balloon_text_color>
      </graph> 

   </graphs>

</settings>
